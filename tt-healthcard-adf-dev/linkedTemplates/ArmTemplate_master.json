{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tt-healthcard-adf-dev"
		},
		"AzureDatabricks_LS_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0305-073826-veci6lvq"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://tt-health002-care-kv.vault.azure.net/"
		},
		"DataHosp_Ellipse_KV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvault-EllipseProject.vault.azure.net/"
		},
		"DataLakeLinkedService_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ttadlsgen22dev.dfs.core.windows.net/"
		},
		"hos_sql_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ttrendytech-hospital-a.database.windows.net"
		},
		"hos_sql_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().db_name}"
		},
		"hos_sql_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "hospadmina"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tt-healthcard-adf-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"DataHosp_Ellipse_KV_properties_typeProperties_baseUrl": {
						"value": "[parameters('DataHosp_Ellipse_KV_properties_typeProperties_baseUrl')]"
					},
					"AzureDatabricks_LS_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_LS_properties_typeProperties_existingClusterId')]"
					},
					"DataLakeLinkedService_ls_properties_typeProperties_url": {
						"value": "[parameters('DataLakeLinkedService_ls_properties_typeProperties_url')]"
					},
					"hos_sql_ls_properties_typeProperties_server": {
						"value": "[parameters('hos_sql_ls_properties_typeProperties_server')]"
					},
					"hos_sql_ls_properties_typeProperties_database": {
						"value": "[parameters('hos_sql_ls_properties_typeProperties_database')]"
					},
					"hos_sql_ls_properties_typeProperties_userName": {
						"value": "[parameters('hos_sql_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}